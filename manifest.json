{
    "manifest_version": 3,
    "name": "Custom New Tab",
    "version": "1.0",
    "description": "Custom new tab page with site shortcuts and daily planner",
    "chrome_url_overrides": {
        "newtab": "tab.html"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "icons": {
        "128": "art/Icon.png"
    }
}
